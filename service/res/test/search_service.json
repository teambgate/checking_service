{
        "request" : "get",
        "path"  : "/supervisor/service/_search",
        "data"  : {
                "query": {
                        "has_child": {
                                "type": "location",
                                "query" : {
                                        "filtered" : {
                                                "filter" : {
                                                    "geo_distance" : {
                                                        "distance" : "2m",
                                                        "location" : {
                                                            "lat" : 100,
                                                            "lon" : 100
                                                        }
                                                    }
                                                }
                                        }
                                }
                        }
                }
        }
}
